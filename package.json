{
  "name": "progress-summary",
  "version": "1.0.0",
  "description": "Summary tool for neo-project, neo-ngd, CityOfZion, NewEconoLab and NeoResearch",
  "scripts": {
    "build": "",
    "precommit": "yarn lint",
    "lint": "eslint 'src/**/*.js*'",
    "start": "parcel src/index.html",
    "test": "jest --all --verbose --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:local": "jest --all --verbose --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/be-neo/progress-summary.git"
  },
  "keywords": [
    "React",
    "Parcel",
    "JSS",
    "JS",
    "HTML",
    "boilerplate",
    "starter"
  ],
  "author": "Jeroen Peeters <contact@jeroenpeeters.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/be-neo/progress-summary/issues"
  },
  "homepage": "https://github.com/be-neo/progress-summary#readme",
  "dependencies": {
    "antd": "3.8.1",
    "babel-polyfill": "6.26.0",
    "jss": "9.8.7",
    "jss-camel-case": "6.1.0",
    "jss-global": "3.0.0",
    "jss-vendor-prefixer": "8.0.1",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-jss": "8.6.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "coveralls": "3.0.2",
    "enzyme": "3.4.3",
    "enzyme-adapter-react-16": "1.2.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "parcel-bundler": "1.9.7",
    "prettier": "1.14.2",
    "raf": "3.4.0",
    "react-test-renderer": "16.4.2"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/__helpers__/TestPropTypes.helper.js",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/src/__mocks__/file.mock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
